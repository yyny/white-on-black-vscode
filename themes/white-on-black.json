{
    "$schema":"vscode://schemas/color-theme",
    "name": "White on Black",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "*.declaration": { "bold": true, "foreground": "#ffaadd" },
		"*.definition": { "bold": true, "foreground": "#ffaadd" },
		"*.readonly": { "foreground": "#4A6FA5" },
		"annotation.declaration": { "bold": true, "foreground": "#FFB070" },
		"annotation.definition": { "bold": true, "foreground": "#FFB070" },
		"event.declaration": { "bold": true, "foreground": "#ffaadd" },
		"event.definition": { "bold": true, "foreground": "#ffaadd" },
		"method.declaration": { "bold": true, "foreground": "#ffaadd" },
		"method.definition": { "bold": true, "foreground": "#ffaadd" },
		"function.declaration": { "bold": true, "foreground": "#ffaadd" },
		"function.definition": { "bold": true, "foreground": "#ffaadd" },
		"macro.declaration": { "bold": true, "foreground": "#ffaadd" },
		"macro.definition": { "bold": true, "foreground": "#ffaadd" },
		"label.declaration": { "bold": true, "foreground": "#ffaadd" },
		"label.definition": { "bold": true, "foreground": "#ffaadd" },
		"property.declaration": { "bold": false },
		"variable.declaration": { "bold": false },
		"parameter.declaration": { "bold": false },
		"number": { "foreground": "#4A6FA5" },
		"string": { "foreground": "#4DA167" },
		"regexp": { "foreground": "#4DA167" },
		"keyword": { "bold": true, "foreground": "#ffffff" },
		"operator": { "foreground": "#ffffff" },
		"type": { "foreground": "#FFB070" },
		"struct": { "foreground": "#FFB070" },
		"class": { "foreground": "#FFB070" },
		"interface": { "foreground": "#FFB070" },
		"enum": { "foreground": "#FFB070" },
		"enumMember": { "foreground": "#4A6FA5" },
		"event": { "foreground": "#73A6D4" },
		"function": { "foreground": "#73A6D4" },
		"method": { "foreground": "#73A6D4" },
		"macro": { "foreground": "#ffffff" },
		"macro.readonly": { "foreground": "#4A6FA5" },
		"label": { "foreground": "#ffffff" },
	},
    "colors": {
		"activityBar.activeBackground": "#0063a5",
		"activityBar.activeBorder": "#ffffff",
		"activityBar.activeFocusBorder": "#0043a5",
        "activityBar.background": "#080808",
        "activityBar.border": "#000000",
        "activityBar.dropBorder": "#ffffff",
		"editor.background": "#000",
		"editor.foreground": "#ccc",
		"editor.lineHighlightBackground": "#040404",
		"editor.selectionBackground": "#282a2e",
		"editorCursor.foreground": "#fff",
        "editorIndentGuide.activeBackground": "#0f0f0f",
        "editorIndentGuide.background": "#060606",
		"editorGutter.background": "#010202",
        "editorWhitespace.foreground": "#222",
		"foreground": "#ccc",
		"panel.background": "#000000",
		"panel.border": "#040404",
        "ports.iconRunningProcessForeground": "#FFFFFF",
        "selection.background": "#080808",
        "sideBar.background": "#040404",
		"sideBar.border": "#000000",
        "sideBarTitle.foreground": "#FFFFFF",
		"sideBarSectionHeader.foreground": "#ffffff",
        "sideBarSectionHeader.background": "#020202",
		"statusBar.background": "#010101",
        "statusBarItem.activeBackground": "#0063a5d0",
		"statusBarItem.hoverBackground": "#00238580",
        "statusBarItem.prominentBackground": "#000000",
        "statusBarItem.prominentHoverBackground": "#00238580",
        "statusBarItem.remoteBackground": "#000000",
		"tab.border": "#040404",
		"tab.activeBackground": "#000000",
		"tab.inactiveBackground": "#161818",
        "window.activeBorder": "#000000",
        "window.inactiveBorder": "#101010",
        "terminal.background": "#000000",
		"terminal.foreground": "#cccccc",
        "terminal.ansiBlack": "#00303b",
        "terminal.ansiRed": "#ff7b7b",
        "terminal.ansiGreen": "#7bff7b",
        "terminal.ansiYellow": "#ffff7b",
        "terminal.ansiBlue": "#7b7bff",
        "terminal.ansiMagenta": "#ff7bff",
        "terminal.ansiCyan": "#7bffff",
        "terminal.ansiWhite": "#a1b1a5",
        "terminal.ansiBrightBlack": "#517171",
        "terminal.ansiBrightRed": "#ff9c9c",
        "terminal.ansiBrightGreen": "#9cff9c",
        "terminal.ansiBrightYellow": "#ffff9c",
        "terminal.ansiBrightBlue": "#9c9cff",
        "terminal.ansiBrightMagenta": "#ff9cff",
        "terminal.ansiBrightCyan": "#9cffff",
        "terminal.ansiBrightWhite": "#f6f6de"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#aaa"
			}
		},
		{
			"name": "Function",
			"scope": [
				"meta.function.declaration",
				"meta.function.definition",
				"meta.function.declaration entity.name.function",
				"meta.function.definition entity.name.function"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffaadd"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage.modifier",
				"entity.name.tag"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Function",
			"scope": "entity.name",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#444444"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"foreground": "#a581af"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
				"variable.other.enummember",
				"punctuation.definition.constant",
				"punctuation.definition.numeric",
				"keyword.other.unit"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#4A6FA5"
			}
		},
		{
			"name": "Constant",
			"scope": "constant.language",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.other",
				"support.function",
				"variable.function",
                "meta.function-call entity.name.function",
                "entity.name.function.call"
			],
			"settings": {
				"foreground": "#73A6D4"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string",
				"punctuation.definition.pragma"
			],
			"settings": {
				"foreground": "#4DA167"
			}
		},
		{
			"name": "Type",
			"scope": [
				"support.type",
				"support.class",
				"entity.name.type",
				"storage.type"
			],
			"settings": {
				"foreground": "#FFB070"
			}
		},
		{
			"name": "Type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "bold"
			}
		},
        // Syntax exceptions for vs-code
        {
			"name": "Dictionary Keys",
			"scope": [
				"keyword.key"
			],
			"settings": {
				"foreground": "#ffffff",
                "fontStyle": "",
			}
		},
        {
			"name": "Imports",
			"scope": [
				"storage.modifier.import"
			],
			"settings": {
				"foreground": "#ffffff",
                "fontStyle": "",
			}
		},
	]
}
